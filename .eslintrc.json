{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": [2, { "ignore": ["^[^.].*"] }],
    "react/sort-comp": [0],
    "radix": ["error", "as-needed"],
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "no-unused-expressions": [2, { "allowTernary": true }]
  },
  "globals": {
    "describe": true,
    "it": true
  }
}
